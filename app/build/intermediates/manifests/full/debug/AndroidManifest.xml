<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assistx.hg.sp"
    android:versionCode="5"
    android:versionName="5.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/mainl"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mainl"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >

        <!-- <receiver android:name="com.assistx.hg.spined.ScreenReceiver"> -->


        <!-- </receiver> -->
        <activity android:name="com.assistx.hg.sp.SplashScreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.USER_PRESENT" />
            </intent-filter>
        </activity>
        <activity android:name="com.assistx.hg.sp.MainActivity" />
        <activity android:name="com.assistx.hg.sp.About" />
        <activity android:name="com.assistx.hg.sp.Graph" />
        <activity android:name="com.assistx.hg.sp.Listdata" />
        <activity android:name="com.assistx.hg.sp.BaseActivity" />
        <activity android:name="com.assistx.hg.sp.firstTime" />
    </application>

</manifest>